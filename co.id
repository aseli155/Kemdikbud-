<script>
   const DATE_LOCALE = 'id';
   const GENERIC_DATEPICKER_OPTIONS = {
      changeMonth: true,
      changeYear: true,
      dateFormat: "dd/mm/yy",
      yearRange: "1900:+nn",
   };

   $(document).ready(function() {
      $(function() {
         $('[data-toggle="tooltip"]').tooltip({
            container: 'body',
            html: true
         });
         $(".datepicker").datepicker(GENERIC_DATEPICKER_OPTIONS);
         $(".click-tooltip-class").click(function() {
            var dataId = $(this).data("hint");
            $(`#${dataId}`).toggle();
         })
      });

      $.ajaxSetup({
         headers: {
            'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
         }
      });

      addRequiredStar();
      generateHint();

      // tinyMCE
      if ($(".myeditor")[0]) {
         const example_image_upload_handler = (blobInfo, progress) => new Promise((resolve, reject) => {
            const xhr = new XMLHttpRequest();
            xhr.withCredentials = false;
            xhr.open('POST', "https://piln.kemdikbud.go.id/upload");
            xhr.upload.onprogress = (e) => {
               progress(e.loaded / e.total * 100);
            };
            xhr.onload = () => {
               if (xhr.status === 403) {
                  reject({
                     message: 'HTTP Error: ' + xhr.status,
                     remove: true
                  });
                  return;
               }
               if (xhr.status < 200 || xhr.status >= 300) {
                  reject('HTTP Error: ' + xhr.status);
                  return;
               }
               const json = JSON.parse(xhr.responseText);
               if (!json || typeof json.location != 'string') {
                  reject('Invalid JSON: ' + xhr.responseText);
                  return;
               }
               resolve(json.location);
            };

            xhr.onerror = () => {
               reject('Image upload failed due to a XHR Transport error. Code: ' + xhr.status);
            };

            const formData = new FormData();
            formData.append('file', blobInfo.blob(), blobInfo.filename());
            xhr.setRequestHeader('X-CSRF-TOKEN', $('meta[name="csrf-token"]').attr('content'));
            xhr.send(formData);
         });

         tinymce.init({
            selector: 'textarea.myeditor',
            path_absolute: "https://piln.kemdikbud.go.id" + "/",
            plugins: [
               "image", "lists"
            ],
            toolbar: "insertfile undo redo | styleselect | bold italic | alignleft aligncenter alignright alignjustify | bullist numlist outdent indent | link image",
            automatic_uploads: true,
            relative_urls: false,
            convert_urls: false,
            images_upload_url: "https://piln.kemdikbud.go.id/upload",
            file_picker_types: 'image',
            images_upload_handler: example_image_upload_handler,
            file_picker_callback: function(cb, value, meta) {
               var input = document.createElement('input');
               input.setAttribute('type', 'file');
               input.setAttribute('accept', 'image/*');

               input.onchange = function() {
                  var file = this.files[0];

                  var reader = new FileReader();
                  reader.onload = function() {
                     var id = 'blobid' + (new Date()).getTime();
                     var blobCache = tinymce.activeEditor.editorUpload.blobCache;
                     var base64 = reader.result.split(',')[1];
                     var blobInfo = blobCache.create(id, file, base64);
                     blobCache.add(blobInfo);

                     /* call the callback and populate the Title field with the file name */
                     cb(blobInfo.blobUri(), {
                        title: file.name
                     });
                  };
                  reader.readAsDataURL(file);
               };

               input.click();
            }
         });
      }

      // swal logout
      $(".link-logout").click(function(e) {
         e.preventDefault();
         swal({
            title: "Apakah anda ingin keluar dari sesi ini?",
            icon: "warning",
            buttons: {
               cancel: {
                  text: "Batal",
                  value: null,
                  visible: true,
                  className: "",
                  closeModal: true,
               },
               confirm: true,
            },
         }).then((value) => {
            if (value) {
               window.location.href = $('.link-logout').attr('href');
            }
         });
      })

      $(document).on("click", ".open-popup", function(e) {
         $(this).magnificPopup({
            type: 'inline',
            showCloseBtn: false,
            callbacks: {
               beforeOpen: function() {
                  if ($(window).width() < 700) {
                     this.st.focus = false;
                  } else {
                     this.st.focus = '#name';
                  }
               }
            }
         }).magnificPopup('open');
      });

      $(document).on("click", ".close-popup", function(e) {
         $.magnificPopup.close();
      });
   });

   function addRequiredStar() {
      let requiredStar = $("<span>")
         .text("*")
         .css("color", "#E74A3B");
      $("label.required").append(requiredStar);
   }

   function initPopupHint() {
      $('.pop-up-hint').magnificPopup({
         type: 'inline',
         preloader: false,
         focus: '#name',

         // When elemened is focused, some mobile browsers in some cases zoom in
         // It looks not nice, so we disable it:
         callbacks: {
            beforeOpen: function() {
               if ($(window).width() < 700) {
                  this.st.focus = false;
               } else {
                  this.st.focus = '#name';
               }
            }
         }
      });
   }

   function generateHint() {
      let selectedLabels = document.getElementsByClassName('need-hint');
      let allFields = [...selectedLabels]
         .map(el => el.htmlFor)
         .filter(fieldName => (fieldName || "").length > 0);
      let uniqueFields = [...new Set(allFields)];

      const createHintModal = (mapLabelHint) => {
         const mapHintModalId = Object.fromEntries(
            Object.keys(mapLabelHint).map(fieldName => [fieldName, `${fieldName}_hint`])
         );
         Object.keys(mapLabelHint).forEach(fieldName => {
            let modal = $("<div>")
               .addClass("mfp-hide white-popup-block")
               .prop("id", mapHintModalId[fieldName])
               .html(`<p>${mapLabelHint[fieldName]}</p>`)
            $("#hint-container").append(modal);
         });
         return mapHintModalId;
      };

      const putHintToLabels = (mapLabelHint, mapHintModalId) => {
         $(selectedLabels).each(function() {
            let fieldName = $(this).prop('for');
            if ((fieldName || "").length > 0) {
               let hintIcon = $("<a>")
                  .addClass(
                     "fa fa-info-circle click-tooltip-class text-dark text-decoration-none pop-up-hint pl-1"
                  )
                  .attr("aria-hidden", true)
                  .prop("href", `#${mapHintModalId[fieldName]}`);
               $(this).append(hintIcon);
            }
         });
      };

      if (uniqueFields.length > 0) {
         $.ajax({
            method: "POST",
            url: "https://piln.kemdikbud.go.id/hint",
            data: {
               field_name: uniqueFields
            },
            success: function(data) {
               let mapHintModalId = createHintModal(data);
               putHintToLabels(data, mapHintModalId);
               initPopupHint();
            }
         });
      }
   }

   function getFullDate(stringDate) {
      const date = new Date(stringDate);

      const month = date.toLocaleString(DATE_LOCALE, {
         month: "long"
      });
      const day = date.getDate();
      const year = date.getFullYear();
      const hour = date.getHours();
      const time = date.getTime();
      const minute = ('0' + date.getMinutes()).slice(-2);
      const second = ('0' + date.getSeconds()).slice(-2);

      return {
         time,
         day,
         month,
         year,
         hour,
         minute,
         second
      }
   }

   function swalOnRequest(isSuccess, successMessage, errorMessage) {

      let swalPayload = {}
      if (isSuccess) {
         swalPayload = {
            title: "Sukses",
            text: successMessage,
            icon: 'success',
         }

      } else {
         swalPayload = {
            title: "Gagal",
            text: errorMessage,
            icon: 'error',
         };

      }
      swal(swalPayload);
   }

   const createImageCropper = () => ({
      modal: null,
      image: null,
      cropper: null,
      outputText: null,
      initModal: function(modalSelector) {
         this.modal = $(modalSelector);

      },
      initImage: function(imageSelector) {
         this.image = document.querySelector(imageSelector);
      },
      initOutput: function(inputTextSelector) {
         this.outputText = $(inputTextSelector);
      },
      initCropper: function(aspectRatio) {
         const that = this;
         this.modal.on('shown.bs.modal', function() {
            that.cropper = new Cropper(that.image, {
               aspectRatio,
               viewMode: 3,
               preview: '.preview'
            });
         }).on('hidden.bs.modal', function() {
            that.cropper.destroy();
            that.cropper = null;
         });
      },
      initInputImage: function(inputImageSelector) {
         const that = this;
         $("body").on("change", inputImageSelector, function(e) {

            const isInputValid = $(inputImageSelector).valid();

            if (!isInputValid) return;

            let files = e.target.files;
            let done = function(url) {
               that.image.src = url;
               that.modal.modal('show');
            };
            let reader;
            let file;
            let url;
            if (files && files.length > 0) {
               file = files[0];
               if (URL) {
                  done(URL.createObjectURL(file));
               } else if (FileReader) {
                  reader = new FileReader();
                  reader.onload = function(e) {
                     done(reader.result);
                  };
                  reader.readAsDataURL(file);
               }
            }
         });
      },
      doCropOnClick: function(buttonSelector, outputSize) {
         const that = this;
         const button = $(buttonSelector);
         button.click(function() {
            canvas = that.cropper.getCroppedCanvas({
               width: outputSize.width,
               height: outputSize.height,
            });
            canvas.toBlob(function(blob) {
               let reader = new FileReader();
               reader.readAsDataURL(blob);
               reader.onloadend = function() {
                  let base64data = reader.result;
                  that.outputText.val(base64data);
                  that.modal.modal('hide');
               }
            });
         });
      }
   });
</script>



